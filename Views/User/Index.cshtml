@model List<User>

@{
    ViewData["Title"] = "Usu√°rios";
}

<ul>
    @foreach (var user in Model)
    {
        <li>
            <p>@user.UserId</p>
            <p>@user.Name</p>
            <p>@user.Email</p>
            <p>@user.Phone</p>
            <a  asp-controller="User" asp-action="Profile" asp-route-id="@user.UserId">Perfil</a>
        </li>
    }
</ul>
